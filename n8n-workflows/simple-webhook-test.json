{
  "name": "Simple GET Test",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "GET",
        "path": "test"
      },
      "id": "webhook1",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [300, 200]
    },
    {
      "parameters": {
        "respondWith": "text",
        "responseBody": "Hello! Webhook working! Time: {{ new Date().toISOString() }}"
      },
      "id": "respond1",
      "name": "Respond",
      "type": "n8n-nodes-base.respondToWebhook", 
      "typeVersion": 1,
      "position": [500, 200]
    }
  ],
  "connections": {
    "Webhook": {
      "main": [[{"node": "Respond", "type": "main", "index": 0}]]
    }
  }
}
