{
  "name": "POST Test with Echo",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "post-test",
        "options": {}
      },
      "id": "webhook1",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [300, 200]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ JSON.stringify($json, null, 2) }}"
      },
      "id": "respond1",
      "name": "Echo Response",
      "type": "n8n-nodes-base.respondToWebhook", 
      "typeVersion": 1,
      "position": [500, 200]
    }
  ],
  "connections": {
    "Webhook": {
      "main": [[{"node": "Echo Response", "type": "main", "index": 0}]]
    }
  }
}
